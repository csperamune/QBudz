<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizapp">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".QBuds_Categories" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Admin_View_Material_List" android:parentActivityName=".Admin_Add_Materials"/>

        <activity android:name=".Admin_View_Materials" android:parentActivityName=".Admin_View_Material_List"/>


        <activity android:name=".User_View_Materials" />
        <activity android:name=".User_View_Material_List"/>

        <activity android:name=".Admin_Add_Materials_Content" android:parentActivityName=".Admin_View_Material_List"/>
        <activity android:name=".Admin_Add_Materials" android:parentActivityName=".QBuds_Categories"/>

        <activity android:name=".Admin_Edit_Materials_Content" android:parentActivityName=".Admin_View_Materials"/>

    </application>

</manifest>